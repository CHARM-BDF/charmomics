{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "VITE_CJS_TRACE=true vite --host 0.0.0.0 --port 80",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint ./",
    "lint:auto": "eslint ./ --fix"
  },
  "dependencies": {
    "vue": "^3.5.12",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "eslint": "^9.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
